{
    "server": {
        "host": "localhost",
        "port": 9080,
        "logLevel": "debug"
    },
    "db": {
        "client": "mssql",
        "connection": {
            "requestTimeout": 30000,
            "options": {
                "encrypt": false,
                "enableArithAbort": true
            }
        },
        "migrations": {
            "tableName": "knex_migrations",
            "directory": "./db/migrations"
        }
    }
}